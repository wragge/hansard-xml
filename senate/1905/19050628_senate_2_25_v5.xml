                    </continue>
                    <interjection>
                        <talk.start>
                            <talker>
                                <page.no>6</page.no>
                                <time.stamp />
                                <name.id>JTV</name.id>
                                <electorate></electorate>
                                <party>ALP</party>
                                <role></role>
                                <in.gov>0</in.gov>
                                <first.speech>0</first.speech>
