    <debate><debateinfo><title>QUESTIONS</title><page.no>1</page.no><type>Questions</type></debateinfo><subdebate.1><subdebateinfo>
          <title>CAPTAIN HERBERT HINKLER, A.F.C</title>
          <page.no>1</page.no>
        </subdebateinfo><speech>
          <talk.start>
            <talker>
              <page.no>1</page.no>
              <time.stamp />
              <name.id>F4B</name.id>
